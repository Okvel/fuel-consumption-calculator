<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="by.liauko.siarhei.cl">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.App"
            android:fullBackupContent="@xml/backup_descriptor">
        <activity android:name=".activity.LogDataActivity" />
        <activity android:name=".activity.FuelDataActivity" />
        <activity
                android:name=".activity.MainActivity"
                android:exported="true"
                android:theme="@style/Theme.App.Starter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CarDataActivity" />
        <activity android:name=".activity.CarProfilesActivity" />
        <activity android:name=".activity.WelcomeActivity" />
    </application>

</manifest>
